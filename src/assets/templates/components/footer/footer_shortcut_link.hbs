{{#each footer_shortcut}}
<div class="col-xs-10 col-xs-offset-1 col-sm-3 col-sm-offset-1">
    <table class="table table-condensed">
        <thead>
            <tr>
                <th>{{this.name}}</th>
            </tr>
        </thead>
        <tbody>
            {{#each category}}
            <tr>
                <td>{{this}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>
{{/each}}